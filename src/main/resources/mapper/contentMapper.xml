<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pikolo.pikolo.mapper.ContentDAO">
    <select id="selectAllContentKO" resultType="com.pikolo.pikolo.dto.ContentDTO">
        SELECT * from content_ko order by content_id desc
    </select>
    <select id="selectAllContentEN" resultType="com.pikolo.pikolo.dto.ContentDTO">
        SELECT * from content_en order by content_id desc
    </select>
    <select id="selectAllContentJP" resultType="com.pikolo.pikolo.dto.ContentDTO">
        SELECT * from content_jp order by content_id desc
    </select>
</mapper>